<!DOCTYPE html><html><head>	<meta charset="utf-8"/>	<title>animate函数封装</title>	<style>		*{			margin: 0px;			padding: 0px;		}		#box{			width: 100px;			height: 100px;			background-color: red;			position:absolute;			left:0;			top: 0;		}	</style>	<script src="2-animate函数.js"></script></head><body>	<div id="box"></div></body><script>	var box=document.getElementById("box");	box.onclick=function(){		animate(box,{left:500,top:300,opacity:20},function(){			console.log(1);		});//匀速运动//		setInterval(function(){//			var now=parseInt(getStyle(box,'left'));//			var now2=parseInt(getStyle(box,'top'));//			if(now==500){//				clearInterval(timer)//			}//			else{//			box.style.left=now + 1+ "px";//			box.style.top=now2 + 1 +"px";//			}//		},30)//变速运动// 		var timer=setInterval(function(){// 			var now=parseInt(getStyle(box,'left'));// 			var speed=(300-now)/6; // 			console.log(now);// 			speed=speed>0?Math.ceil(speed):Math.floor(speed);// 			console.log(now);// 			if(now==300){//				clearInterval(timer)//			}//			else{//				box.style.left=now + speed+ "px";//			}// 		},30)	}		</script><[表情]ml>

2018-10-08 10:35:10 鲸鱼🐳
function getStyle(obj,attr){	if(obj.currentStyle){		return obj.currentStyle[attr];	}	else{		return getComputedStyle(obj,null)[attr];	}}function animate(obj,json,callback){	clearInterval(obj.timer);	obj.timer=setInterval(function(){		console.log(1);		var isStop=true;  //标志位，有一个没到就不清除		for(var attr in json){			var now=0;			if(attr=='opacity'){				now=parseInt(getStyle(obj,attr)*100);			}			else{				now=parseInt(getStyle(obj,attr));			}			var speed=(json[attr] - now)/6;    			speed=speed>0?Math.ceil(speed):Math.floor(speed);   			var current=now + speed;   			if(attr=='opacity'){   				obj.style.opacity=current/100;			}   			else{   				obj.style[attr]=current+ "px";   			}			if(json[attr] !== current){				isStop=false;			}		}		if(isStop){			clearInterval(obj.timer);			if(callback){callback()};		}			},30)}

