import React, { Component } from 'react';import Comment from './Comment';import DateSource from '../ds/DateSource';export default class BlogPost extends Component {    constructor(){        super();        this.state = {            comments:DateSource.getBlogPost()        }    }    componentDidMount() {        DateSource.addChangeListener(this.handleChange);    }    handleChange = ()=>{        this.setState({            comments:DateSource.getBlogPost()        })    }    render() {        return (            <ul>                {                    this.state.comments.map((item,index)=>(                        <Comment comment={item} key={index}/>                    ))                }                            </ul>        )    }}

2019-03-14 9:34:09 乐一
import React, { Component } from 'react';import Comment from './Comment';import DateSource from '../ds/DateSource';function withSubscription(WrappedComponent, selectData) {    return class extends React.Component {      constructor(props) {        super(props);        this.handleChange = this.handleChange.bind(this);        this.state = {          data: selectData(DateSource, props)        };      }        componentDidMount() {        DateSource.addChangeListener(this.handleChange);      }        componentWillUnmount() {        DateSource.removeChangeListener(this.handleChange);      }        handleChange() {        this.setState({          data: selectData(DateSource, this.props)        });      }        render() {        return <WrappedComponent data={this.state.data} {...this.props} />;      }    };  }  export default withSubscription;

