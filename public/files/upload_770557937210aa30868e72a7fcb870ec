import React ,{Component} from 'react';import ReactDOM from 'react-dom';import './index.css';//import Dots from './Dots';//import Slide from './Slide';//import App from './App';//import Types from './Types';//import Controlled from './Controll';import axios from 'axios';class App extends Component {    constructor(){        super();        this.state={            authors:'',            content:'',            title:''        }    }    componentDidMount=()=>{        axios.get('https://api.apiopen.top/recommendPoetry')        .then((res)=>{            let { authors,content,title}=res.data.result;            this.setState({                authors,content,title            })        })    }    render(){        return <h1>{this.state.title}</h1>    }}ReactDOM.render(<App />,document.getElementById('root'));

